<!DOCTYPE html>
<html>
<head>
    {% if includejQuery %}
        <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/themes/smoothness/jquery-ui.css">
    {% endif %}
    {% if includeBootstrap %}
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    {% endif %}
    {% if includeAssets %}
        {{ include("@IpPage/Page/helper/assets_css.html.twig") }}
    {% endif %}
</head>
<body>
{% if includejQuery %}
    <script src="//code.jquery.com/jquery-3.1.1.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js"></script>
{% endif %}
{% if includeBootstrap %}
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
{% endif %}
{% if includeAssets %}
    {{ include("@IpPage/Page/helper/assets_js.html.twig") }}
{% endif %}
<script type="text/javascript" charset="utf-8">
    $(document).ready(function () {
        $('#page').pagemaker({
            id_element: $(this).data('idelement'),
            "pluginRoot": "http://api.iboutiq.com/web/assetspage/plugins/pagemaker/",
            "saveURL": "{{ path('ip_page_save') }}",
            "url_biblio": "{{ path('ip_bibliotheque_folders') }}",
            "url_upload": "{{ path('ip_bibliotheque_add') }}"
        });
    });
</script>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div id="page" data-idelement="1"></div>
        </div>
    </div>
</div>
</body>
</html>
